<h1>Listing my_test_objects</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Cost</th>
    <th>Is active</th>
    <th>Desc</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @my_test_objects.each do |my_test_object| %>
  <tr>
    <td><%= my_test_object.name %></td>
    <td><%= my_test_object.cost %></td>
    <td><%= my_test_object.is_active %></td>
    <td><%= my_test_object.desc %></td>
    <td><%= link_to 'Show', my_test_object %></td>
    <td><%= link_to 'Edit', edit_my_test_object_path(my_test_object) %></td>
    <td><%= link_to 'Destroy', my_test_object, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

